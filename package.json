{
  "name": "wh3mm",
  "productName": "wh3mm",
  "version": "1.26.2",
  "description": "WH3 Mod Manager",
  "main": ".webpack/main",
  "scripts": {
    "start": "tsc ./src/sub.ts ./src/readPacksWorker.ts --resolveJsonModule --outDir ./temp && electron-forge start",
    "package": "electron-forge package",
    "make": "tsc ./src/sub.ts ./src/readPacksWorker.ts --resolveJsonModule --outDir ./temp && electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "keywords": [],
  "author": {
    "name": "Tibor IliÄ‡",
    "email": "tilic5@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "Shazbot",
              "name": "WH3-Mod-Manager"
            }
          }
        }
      ],
      "makers": [
        {
          "name": "@electron-forge/maker-zip"
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/preload.ts"
                  }
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@electron-forge/cli": "^6.0.0-beta.64",
    "@electron-forge/maker-deb": "^6.0.0-beta.64",
    "@electron-forge/maker-rpm": "^6.0.0-beta.64",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.64",
    "@electron-forge/maker-zip": "^6.0.0-beta.64",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.64",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/jest": "^0.0.10",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@types/fs-extra": "^9.0.13",
    "@types/nightmare": "^2.10.6",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/winreg": "^1.2.31",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.0",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "electron": "19.0.4",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.18.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-storybook": "^0.5.12",
    "fork-ts-checker-webpack-plugin": "^7.2.11",
    "node-loader": "^2.0.0",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.0",
    "style-loader": "^3.3.1",
    "tailwind-scrollbar": "^2.0.1",
    "tailwindcss": "^3.1.3",
    "ts-loader": "^9.3.0",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "@floating-ui/react-dom": "^1.0.0",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@heroicons/react": "^2.0.12",
    "@node-steam/vdf": "^2.2.0",
    "@reduxjs/toolkit": "^1.8.2",
    "@types/binary-file": "^0.2.0",
    "binary-file": "^0.2.3",
    "chokidar": "^3.5.3",
    "classnames": "^2.3.1",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.3.6",
    "electron-fetch": "^1.7.4",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.8",
    "electron-prebuilt-compile": "^8.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-window-state": "^5.0.3",
    "fast-deep-equal": "^3.1.3",
    "flowbite": "^1.4.7",
    "flowbite-react": "^0.0.27",
    "fs-extra": "^10.1.0",
    "gh-release-fetch": "^3.0.2",
    "just-clone": "^6.1.1",
    "just-debounce-it": "^3.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-joyride": "^2.5.0",
    "react-redux": "^8.0.2",
    "react-select": "^5.3.2",
    "semver": "^7.3.7",
    "steamworks.js": "^0.0.21",
    "winreg": "^1.2.4"
  }
}
