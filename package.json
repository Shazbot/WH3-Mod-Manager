{
  "name": "wh3mm",
  "productName": "wh3mm",
  "version": "2.12.2",
  "description": "WH3 Mod Manager",
  "main": ".webpack/main",
  "scripts": {
    "start": "tsc ./src/sub.ts ./src/readPacksWorker.ts --skipLibCheck --resolveJsonModule --outDir ./temp && electron-forge start --inspect-electron",
    "package": "electron-forge package",
    "make": "tsc ./src/sub.ts ./src/readPacksWorker.ts --skipLibCheck --resolveJsonModule --outDir ./temp && electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "storybook": "echo 'storybook currently broken, storybook dev -p 6006'",
    "build-storybook": "storybook build"
  },
  "keywords": [],
  "author": {
    "name": "Tibor IliÄ‡",
    "email": "tilic5@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./src/assets/modmanager.ico"
      },
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "Shazbot",
              "name": "WH3-Mod-Manager"
            }
          }
        }
      ],
      "makers": [
        {
          "name": "@electron-forge/maker-zip"
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        {
          "name": "@electron-forge/plugin-webpack",
          "config": {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.ts",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/preload.ts"
                  }
                },
                {
                  "html": "./src/viewer/viewerIndex.html",
                  "js": "./src/renderer.ts",
                  "name": "viewer",
                  "preload": {
                    "js": "./src/preload.ts"
                  }
                },
                {
                  "html": "./src/viewer/viewerIndex.html",
                  "js": "./src/renderer.ts",
                  "name": "skills",
                  "preload": {
                    "js": "./src/preload.ts"
                  }
                }
              ]
            }
          }
        }
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@chromatic-com/storybook": "3",
